{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["DOM", "ES2020"],
    "moduleResolution": "bundler",
    "strict": false,
    "allowJs": false,
    "baseUrl": "./src",
    "rootDir": "src",
    "outDir": "dist",
    "skipLibCheck": true,
    "removeComments": false,
    "sourceMap": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
}
